      function saltarL() {
        document.write("<br>");
        document.write("<br>");
        document.write("<hr>");
        document.write("<br>");
        document.write("<br>");
      }
      function write(frase) {
        document.write(frase);
        saltarL();
      }

      var numeroP = Math.round(Math.random() * 10);
      var numeroU = parseInt(prompt("Digite un numero de 1 a 10"));
      if (numeroU == numeroP) {
        write("Usted ha acertado el numero era: " + numeroP);
      }
        write("Usted err√≥" + " el numero era: " + numeroP);
  
